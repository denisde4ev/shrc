if command. exa; then
# small note: the reason `alias 'l'` prints '.' by doulbe `-aa` is because when I run it in empty dir and get no output, I panic.
export LSLONG_COMMAND="exa --sort=none -alF --git --time-style long-iso --group-directories-first -g"
alias \
ls='exa -a1F --group-directories-first' \
l='exa -alF --git --time-style iso --group-directories-first -a' \
lll=`:   `'exa -alF --git --time-style full-iso --group-directories-first --hyperlink --icons -a -g --header --links --inode' \
llT=`:   `'exa -alF       --time-style iso      --group-directories-first                                                    -Th' \
lllT=`:  `'exa -alF --git --time-style full-iso --group-directories-first --hyperlink --icons -g    --header --links --inode -Th' \
lllT0='lllT --level 1' lllT1='lllT --level 1' lllT2='lllT --level 2' lllT3='lllT --level 3' lllT4='lllT --level 4' lllT5='lllT --level 5' \
llT0='llT --level 0'   llT1='llT --level 1'   llT2='llT --level 2'   llT3='llT --level 3'   llT4='llT --level 4'   llT5='llT --level 5'   \
;
else
export LSLONG_COMMAND='ls -alF --group-directories-first'
alias \
ls='\ls -F --color=auto -1 --group-directories-first' \
l='ll' \
;
fi

if { command. apk ||  command. apt; } && command. pacapt; then
	alias p='pacapt'
elif command. pacman; then
	alias \
		rm.pacmanlock='rm /var/lib/pacman/db.lck' \
		pacman-next='fakeroot pacman -vvv --dbpath /var/lib/pacman-next' \
		cleanup='pacman -Rns $(pacman -Qtdq) && rm -rv /var/lib/systemd/coredump  /var/log/journal/*' \
	;
	if command. trizen; then
		alias p='trizen'
	else
		alias p='pacman'
	fi
	alias yay='p'
fi

