exit 1; return ; eval ')))))))))))))))))' # DON''T RUN me

if test "$PS1"; then
	if test "$BASH" && \
		test -z "$POSIXLY_CORRECT" && \
		test "${0#-}" != sh && \
		test -r /etc/bash.bashrc
	then
		. /etc/bash.bashrc
	else
		if [ -e ~/.config/sh/profile ] && [ ! -e ~/.profile ]
		then . ~/.config/sh/profile
		fi
	fi
fi
